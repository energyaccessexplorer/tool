<script>
  if (location.hostname.match(/localhost/))
    ENV = ['production', 'test', 'dev'];
  else {
    const subdomain = location.hostname.match(/^(.*).energyaccessexplorer.org/)[1];

    switch (subdomain) {
    case 'www':
      ENV = 'production';
      break;

    default:
      ENV = subdomain;
      break;
    }
  }
</script>
